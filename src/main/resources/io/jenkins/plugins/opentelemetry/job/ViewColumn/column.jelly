<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout">
<td>
    <j:forEach items="${it.getLinks(job)}" var="link" varStatus="loop">
        <span class="icon-md">
            <l:icon class="${link.iconClass} icon-md" tooltip="${link.label}" />
        </span>
    </j:forEach>
</td>
</j:jelly>
